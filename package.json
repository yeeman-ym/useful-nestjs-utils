{
  "name": "useful-nestjs-utils",
  "version": "1.0.7",
  "description": "A collection of useful decorators for NestJS applications.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./decorators": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --watch",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeeman-ym/useful-nestjs-utils.git"
  },
  "keywords": [
    "typescript",
    "nestjs"
  ],
  "author": "ym",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeeman-ym/useful-nestjs-utils/issues"
  },
  "homepage": "https://github.com/yeeman-ym/useful-nestjs-utils#readme",
  "dependencies": {
    "@nestjs/common": "^10.3.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/swagger": "^7.2.0",
    "@swc/core": "^1.3.107",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.3.1",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "prettier": "3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
